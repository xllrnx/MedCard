<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="850.0"
            styleClass="temperature_sheet_container" stylesheets="@../../Styles/Doctor/DashboardStyles/DoctorTemperatureSheet.css"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.medcard.Controllers.Doctor.DashboardsControllers.DoctorTemperatureSheetController">
   <HBox alignment="CENTER" layoutX="29.0" layoutY="40.0"
         prefHeight="43.0" prefWidth="790.0" spacing="15.0"
         styleClass="selected_patient"
         AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="30.0">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Обраний пацієнт:" wrappingWidth="150"/>
      <Text fx:id="selected_patient_surname" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Прізвище"
            wrappingWidth="85"/>
      <Text fx:id="selected_patient_name" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text" text="Ім'я"
            wrappingWidth="40"/>
      <Text fx:id="selected_patient_fathername" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text"
            text="По-батькові" wrappingWidth="100"/>
   </HBox>
</AnchorPane>
